{
  "name": "activity-boilerplate",
  "version": "3.0.0",
  "description": "脚手架",
  "main": "index.js",
  "scripts": {
    "test:app": "cross-env NODE_ENV=development mocha ./test/actions/**/*.spec.js ./test/mutations/**/*.spec.js --bail --compilers js:babel-core/register -t 5000 -s 1000",
    "test:api": "cross-env NODE_ENV=production mocha ./test/api/**/*.spec.js --bail --compilers js:babel-core/register -t 5000 -s 1000",
    "test:api:watch": "cross-env NODE_ENV=production mocha ./test/api/**/*.spec.js --bail --compilers js:babel-core/register -t 5000 -s 1000 --watch -R spec",
    "lint": "eslint ./src --fix",
    "doc": "jsdoc -a all -c ./build/jsdoc.config.json ./src -R ./README.md",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.development.js --inline --hot --host 0.0.0.0",
    "pre": "cross-env NODE_ENV=preview webpack --config ./build/webpack.config.preview.js --colors --progress",
    "release": "yarn run lint && cross-env NODE_ENV=production webpack --config ./build/webpack.config.production.js --colors --progress"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "fe@knowbox",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "axios": "^0.15.3",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-jsx": "^6.13.0",
    "babel-plugin-transform-merge-sibling-variables": "^6.8.0",
    "babel-plugin-transform-minify-booleans": "^6.8.0",
    "babel-plugin-transform-node-env-inline": "^6.8.0",
    "babel-plugin-transform-property-literals": "^6.8.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-plugin-transform-remove-debugger": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-vue-jsx": "^3.1.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.12.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "in-view": "^0.6.1",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.2",
    "mocha": "^3.2.0",
    "nock": "^9.0.2",
    "normalize.css": "^4.2.0",
    "postcss-loader": "^1.2.0",
    "precss": "^1.4.0",
    "transfer-webpack-plugin": "^0.1.4",
    "vue": "^2.0.3",
    "vue-gesture": "^1.0.8",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^8.5.2",
    "vue-router": "^2.0.2",
    "vue-style-loader": "^1.0.0",
    "vuex": "^2.0.0",
    "webpack": "^1.13.3",
